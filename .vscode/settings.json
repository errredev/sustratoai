	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				// Configuraci√≥n espec√≠fica para el componente <Box>
				"scope": "entity.name.tag.jsx",
				"settings": {
					"foreground": "#4CAF50", // Verde para <Box>
					"fontStyle": "bold" // Negrita para diferenciarlo
				}
			},
			{
				// Props espec√≠ficas de Box (row, gap)
				"scope": "meta.tag.structure.jsx entity.other.attribute-name.jsx",
				"settings": {
					"foreground": "#FF9800", // Naranja para props de Box
					"fontStyle": "italic"
				}
			},
			{
				// Valores de props (solo cadenas dentro de Box)
				"scope": "meta.tag.structure.jsx string.quoted.double.jsx",
				"settings": {
					"foreground": "#3fb55b" // Azul para cadenas
				}
			},
			{
				// S√≠mbolos espec√≠ficos de <Box> (como < y >)
				"scope": "meta.tag.structure.jsx punctuation.definition.tag.jsx",
				"settings": {
					"foreground": "#9C27B0" // Morado para s√≠mbolos
				}
			},
			{
				"scope": "meta.preprocessor",
				"settings": {
					"foreground": "#FF9800" // Color naranja para #region
				}
			},
			{
				"scope": "comment",
				"settings": {
					"foreground": "#adff9953" // Color verde para comentarios
				}
			},
			{
				"scope": "string",
				"settings": {
					"foreground": "#500deb",
					"fontStyle": "italic bold "
				}
			},
			{
				"scope": "keyword.control.export",
				"settings": {
					"foreground": "#1E90FF",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "keyword.control.function",
				"settings": {
					"foreground": "#0eee33",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "keyword.modifier.default",
				"settings": {
					"foreground": "#fafa8e",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "keyword.control.import",
				"settings": {
					"foreground": "#B0E0E6", // Color vino tinto para imports
					"fontStyle": "italic bold underline"
				}
			},
			{
				"scope": "keyword.control.from",
				"settings": {
					"foreground": "#B0E0E6", // Color vino tinto para imports
					"fontStyle": "italic bold underline"
				}
			},
			{
				"scope": "keyword.control.const",
				"settings": {
					"foreground": "#1eff56", // Azul piedra para const
					"fontStyle": "bold"
				}
			},
			{
				"scope": "keyword.control.const",
				"settings": {
					"foreground": "#1eff56", // Azul piedra para const
					"fontStyle": "bold"
				}
			}
		]
	},
